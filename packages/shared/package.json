{
  "name": "@sales_ai_automation_v3/shared",
  "version": "0.2.0",
  "description": "Shared types, errors, and schemas for Sales AI Automation V3",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./errors": {
      "types": "./dist/errors/index.d.ts",
      "import": "./dist/errors/index.js"
    },
    "./product-configs": {
      "types": "./dist/product-configs/index.d.ts",
      "import": "./dist/product-configs/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js"
    },
    "./types/conversation": {
      "types": "./dist/types/conversation/index.d.ts",
      "import": "./dist/types/conversation/index.js"
    },
    "./types/transcription": {
      "types": "./dist/types/transcription/index.d.ts",
      "import": "./dist/types/transcription/index.js"
    },
    "./types/meddic": {
      "types": "./dist/types/meddic/index.d.ts",
      "import": "./dist/types/meddic/index.js"
    },
    "./types/slack": {
      "types": "./dist/types/slack/index.d.ts",
      "import": "./dist/types/slack/index.js"
    },
    "./types/queue": {
      "types": "./dist/types/queue/index.d.ts",
      "import": "./dist/types/queue/index.js"
    },
    "./types/opportunity": {
      "types": "./dist/types/opportunity/index.d.ts",
      "import": "./dist/types/opportunity/index.js"
    },
    "./schemas": {
      "types": "./dist/schemas/index.d.ts",
      "import": "./dist/schemas/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "check-types": "tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "lint": "bun x ultracite check",
    "fix": "bun x ultracite fix"
  },
  "dependencies": {
    "zod": "catalog:"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "typescript": "catalog:"
  },
  "keywords": [
    "shared",
    "types",
    "errors",
    "schemas"
  ],
  "author": "",
  "license": "ISC"
}
